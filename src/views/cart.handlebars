<h1>Products in cart</h1>

{{#if noEmpty}}
    <div class="product-list">
        {{#each products}}
            <div class="product">
                <h2>{{this.title}}</h2>
                <p>{{this.description}}</p>
                <p>Code: {{this.code}}</p>
                <p>Price: ${{this.price}}</p>
                <p>Stock: {{this.stock}}</p>
                <p>Category: {{this.category}}</p>
            </div>
        {{/each}}
    </div>
    {{else}}
    <h1>Cart is empty</h1>
{{/if}}