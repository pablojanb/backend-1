<h1>Productos en carrito</h1>

{{#if noEmpty}}
    {{#each products}}
        <div id="product">
            <h2>{{this.title}}</h2>
            <p>{{this.description}}</p>
            <p>Code: {{this.code}}</p>
            <p>Price: ${{this.price}}</p>
            <p>Stock: {{this.stock}}</p>
            <p>Category: {{this.category}}</p>
        </div>
    {{/each}}
    {{else}}
    <div>Carrito vacio</div>
{{/if}}